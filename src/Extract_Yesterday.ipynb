# This will extract the data from yesterday.

from datetime import datetime
import requests
import zipfile

import pandas as pd
import matplotlib
import geopandas as gpd
import numpy as np
import matplotlib.pyplot as plt


root_url = "http://aisdata.ais.dk/"
 # This is TODAY, but data is upload daily RETROACTIVELY
value_datetime = str(datetime.yesterday()).split()[0]  
# Since data is uploaded at the end of day, we most work with data the previous day.




half_string = "aisdk-"
filename = half_string + value_datetime  + ".zip"
URL_RawData_Today = root_url + filename
print(URL_RawData_Today)




response = requests.get(URL_RawData_Today)
with open(filename, "wb") as f:
    f.write(response.content)
print("Download complete!")
